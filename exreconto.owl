<?xml version="1.0"?>
<rdf:RDF xmlns="http://gaia.fdi.ucm.es/ontologies/exreconto.owl"
     xml:base="http://gaia.fdi.ucm.es/ontologies/exreconto.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:exreconto="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#">
    <owl:Ontology rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl">
        <owl:versionIRI rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl"/>
        <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl"/>
        <dc:creator>Marta Caro-Martínez, Guillermo Jiménez-Díaz, Juan A. Recio-García</dc:creator>
        <dc:description>ExRecOnto is an ontology to categorize explanations in recommender systems.</dc:description>
        <dc:language>English</dc:language>
        <dc:title>ExRecOnto</dc:title>
        <rdfs:comment>ExRecOnto is an ontology to categorize explanations in recommender systems.</rdfs:comment>
        <owl:versionInfo>1.0.0</owl:versionInfo>
    </owl:Ontology>
    


