    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#builds -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#builds">
        <owl:inverseOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBuiltBy"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Low-Level"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Top-Level"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#defines -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#defines">
        <owl:inverseOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#dependsOn"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#User"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#delinates -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#delinates">
        <owl:inverseOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#showsAccordingTo"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Presentation"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#dependsOn -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#dependsOn">
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#User"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#determinates -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#determinates">
        <owl:inverseOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isDeterminatedBy"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Format"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Knowledge_Containers"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Detail"/>
        <rdfs:label>determines</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#influences -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#influences">
        <owl:inverseOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isInfluenced"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Argumentation"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBaseOf">
        <owl:inverseOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBasedOn"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Generation"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Algorithm"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBasedOn -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBasedOn">
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Algorithm"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Generation"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBuiltBy -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isBuiltBy">
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Top-Level"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Low-Level"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isDeterminatedBy -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isDeterminatedBy">
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Detail"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Format"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Knowledge_Containers"/>
        <rdfs:label>isDeterminedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isInfluenced -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isInfluenced">
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Argumentation"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
        <rdfs:label>isInfluencedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isUsedBy -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isUsedBy">
        <owl:inverseOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#uses"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Data"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Generation"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#showsAccordingTo -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#showsAccordingTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Presentation"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
    </owl:ObjectProperty>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#uses -->

    <owl:ObjectProperty rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#uses">
        <rdfs:domain rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Generation"/>
        <rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Data"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Algorithm -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Algorithm">
        <rdfs:subClassOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Knowledge_Containers"/>
        <rdfs:comment>The explanations are based on the information that the system collects from the recommendation algorithm. The algorithm itself is a knowledge container.</rdfs:comment>
        <rdfs:label>Algorithm</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Argumentation -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Argumentation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Comparison"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Polarity"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>Explanations can show different outlooks for a recommended item.</rdfs:comment>
        <rdfs:label>Argumentation</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Comparison -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Comparison">
        <rdfs:comment>Arguments can be supported using the similarities or the differences between item features and user preferences.</rdfs:comment>
        <rdfs:label>Comparison</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Data -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Data">
        <rdfs:subClassOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Knowledge_Containers"/>
        <rdfs:comment>These are the explanations based on the information available at the recommender system.</rdfs:comment>
        <rdfs:label>Data</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Detail -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Detail">
        <rdfs:comment>The degree of specification about a recommendation included in its explanation.</rdfs:comment>
        <rdfs:label>Detail</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Encapsulation -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Encapsulation"/>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#ExplanationSystem -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#ExplanationSystem">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Generation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Knowledge_Containers"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Motivation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Presentation"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Format -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Format">
        <rdfs:comment>The explanations can be shown in different ways, more or less simple.</rdfs:comment>
        <rdfs:label>Format</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Generation -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Generation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Encapsulation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Technique"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>The way to implement the explanation system.</rdfs:comment>
        <rdfs:label>Generation</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal">
        <rdfs:comment>The goal of an explanation is closely related to the user to whom the explanation is targeted.</rdfs:comment>
        <rdfs:label>Goal</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Interaction -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Interaction">
        <rdfs:comment>The different ways of interaction depend on how the user obtains explanations through the system.</rdfs:comment>
        <rdfs:label>Interaction</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Knowledge_Containers -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Knowledge_Containers">
        <rdfs:comment>Explanations for recommender systems can be created based on different types of knowledge sources or containers.</rdfs:comment>
        <rdfs:label>Knowledge</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Low-Level -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Low-Level">
        <rdfs:subClassOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
        <rdfs:comment>The low-level goals help to achieve the top-level goals.</rdfs:comment>
        <rdfs:label>Low-Level_Goal</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Motivation -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Motivation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#User"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>Motivation refers to the goal of the explanation according to the target user.</rdfs:comment>
        <rdfs:label>Motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Polarity -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Polarity">
        <rdfs:comment>Explanations can show different outlooks for a recommended item: they can present the positive arguments, or they can disclose the negative arguments of this item for the user.</rdfs:comment>
        <rdfs:label>Polarity</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Presentation -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Presentation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Argumentation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Detail"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Format"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#isComposedBy"/>
                <owl:someValuesFrom rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Interaction"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>The way the system displays the explanation to the user.</rdfs:comment>
        <rdfs:label>Presentation</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Technique -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Technique">
        <rdfs:comment>Explanation systems must be also classified according to the algorithm used for generating explanations independently of their introspective or external nature.</rdfs:comment>
        <rdfs:label>Technique</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Top-Level -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Top-Level">
        <rdfs:subClassOf rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Goal"/>
        <rdfs:comment>The top-level goals are focused on the user.</rdfs:comment>
        <rdfs:label>Top-Level_Goal</rdfs:label>
    </owl:Class>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#User -->

    <owl:Class rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#User">
        <rdfs:comment>Recommender systems are supposed to satisfy user needs. However, explanations do not have to be aimed exclusively at the final user.</rdfs:comment>
        <rdfs:label>User</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Ad-hoc -->

    <owl:NamedIndividual rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Ad-hoc">
        <rdf:type rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Technique"/>
    </owl:NamedIndividual>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Background_Information -->

    <owl:NamedIndividual rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Background_Information">
        <rdf:type rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Data"/>
    </owl:NamedIndividual>
    


    <!-- http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Community -->

    <owl:NamedIndividual rdf:about="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Community">
        <rdf:type rdf:resource="http://gaia.fdi.ucm.es/ontologies/exreconto.owl#Data"/>
